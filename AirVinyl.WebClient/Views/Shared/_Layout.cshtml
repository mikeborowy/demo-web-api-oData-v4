                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                @Html.ActionLink("Example 1 |", "Example1", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 2 |", "Example2", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 3a |", "Example3a", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 3b |", "Example3b", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 4 |", "Example4", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 5 |", "Example5", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 6 |", "Example6", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                <br />
                @Html.ActionLink("Example 7 |", "Example7", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 8 |", "Example8", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Example 9 |", "Example9", "AirVinyl", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>